{
    "name": "jmrashed/two-factor-auth",
    "description": "Enhance your Laravel application's security with the Laravel Two-Factor Authentication (2FA) package",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "laravel",
        "authentication",
        "2fa",
        "two-factor",
        "authentication",
        "laravel 11",
        "stable"
    ],
    "authors": [
        {
            "name": "Md Rasheduzzaman",
            "email": "jmrashed@gmail.com",
            "homepage": "https://github.com/jmrashed"
        }
    ],
    "support": {
        "source": "https://github.com/jmrashed/TwoFactorAuth",
        "issues": "https://github.com/jmrashed/TwoFactorAuth/issues"
    },
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "laragear/meta-model": "^1.1",
        "laragear/meta": "3.*",
        "bacon/bacon-qr-code": "2.*",
        "paragonie/constant_time_encoding": "^2.6",
        "illuminate/config": "10.*|11.*",
        "illuminate/validation": "10.*|11.*",
        "illuminate/database": "10.*|11.*",
        "illuminate/support": "10.*|11.*",
        "illuminate/http": "10.*|11.*",
        "illuminate/auth": "10.*|11.*"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Jmrashed\\TwoFactorAuth\\": "src",
            "Database\\Factories\\Jmrashed\\TwoFactorAuth\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --coverage-clover build/logs/clover.xml",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jmrashed\\TwoFactorAuth\\TwoFactorServiceProvider"
            ]
        }
    },
    "funding": [
    ]
}
